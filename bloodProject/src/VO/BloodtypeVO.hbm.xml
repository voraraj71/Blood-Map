<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="VO.BloodtypeVO" table="bloodtypeTable">
      <id name="bloodtypeId" type="int" column="BloodtypeId">
         <generator class="increment"/>
      </id>
      <property name="bloodType">
      <column name="BloodType"/></property>

	<property name="bloodtypeDescription">
      <column name="BloodtypeDescription"/></property>
      
      <property name="deleteStatus">
      <column name="deleteStatus"/></property>
      
	
   </class>
</hibernate-mapping>